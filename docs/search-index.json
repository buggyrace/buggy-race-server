---
layout: null
---
[
{% for page in site.html_pages %}
    {% if want_comma %},{% endif %}{
      "name": {{page.url | jsonify}},
      "link": "{{ site.baseurl }}{{ page.url | replace: 'index.html', '' | replace: '.html', '' }}"
      "title": {{page.title | jsonify}},
      "content": {{page.content | strip_html | jsonify}}
    }
  {% assign want_comma = true %}
{% endfor %}
]